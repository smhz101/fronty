{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        { "color": "#ffffff", "name": "Base", "slug": "base" },
        { "color": "#0f172a", "name": "Contrast", "slug": "contrast" },
        { "color": "#2563eb", "name": "Primary", "slug": "primary" },
        { "color": "#1e40af", "name": "Secondary", "slug": "secondary" },
        { "color": "#f8fafc", "name": "Neutral 50", "slug": "neutral-50" },
        { "color": "#f1f5f9", "name": "Neutral 100", "slug": "neutral-100" },
        { "color": "#e2e8f0", "name": "Neutral 200", "slug": "neutral-200" },
        { "color": "#cbd5e1", "name": "Neutral 300", "slug": "neutral-300" },
        { "color": "#94a3b8", "name": "Neutral 400", "slug": "neutral-400" },
        { "color": "#64748b", "name": "Neutral 500", "slug": "neutral-500" },
        { "color": "#475569", "name": "Neutral 600", "slug": "neutral-600" },
        { "color": "#334155", "name": "Neutral 700", "slug": "neutral-700" },
        { "color": "#1e293b", "name": "Neutral 800", "slug": "neutral-800" },
        { "color": "#0f172a", "name": "Neutral 900", "slug": "neutral-900" }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(135deg, #2563eb 0%, #1e40af 100%)",
          "name": "Electric Blue",
          "slug": "electric-blue"
        },
        {
          "gradient": "linear-gradient(135deg, #0f172a 0%, #1e293b 100%)",
          "name": "Midnight Blue",
          "slug": "midnight-blue"
        },
        {
          "gradient": "linear-gradient(135deg, #f1f5f9 0%, #cbd5e1 100%)",
          "name": "Soft Slate",
          "slug": "soft-slate"
        },
        {
          "gradient": "linear-gradient(135deg, #2563eb 0%, #0f172a 100%)",
          "name": "Deep Ocean",
          "slug": "deep-ocean"
        }
      ],
      "duotone": [
        {
          "colors": ["#0f172a", "#ffffff"],
          "name": "Dark & Light",
          "slug": "dark-and-light"
        },
        {
          "colors": ["#2563eb", "#ffffff"],
          "name": "Blue & White",
          "slug": "blue-and-white"
        },
        {
          "colors": ["#1e40af", "#cbd5e1"],
          "name": "Blue & Slate",
          "slug": "blue-and-slate"
        }
      ]
    },
    "custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 425,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"body": 1.5,
				"relaxed": 1.625,
				"loose": 2
			}
		},
    "layout": {
			"contentSize": "740px",
			"wideSize": "1260px"
		},
		"shadow": {
			"defaultPresets": false,
      "presets": [
        {
          "name": "Extra Large Dark",
          "slug": "extra-large-dark",
          "shadow": "0px 536px 150px 0px rgba(20, 17, 31, 0.00), 0px 343px 137px 0px rgba(20, 17, 31, 0.01), 0px 193px 116px 0px rgba(20, 17, 31, 0.05), 0px 86px 86px 0px rgba(20, 17, 31, 0.09), 0px 21px 47px 0px rgba(20, 17, 31, 0.10)"
        },
				{
          "name": "Large Dark",
          "slug": "large-dark",
          "shadow": "0px 219px 61px 0px rgba(20, 17, 31, 0.00), 0px 140px 56px 0px rgba(20, 17, 31, 0.01), 0px 79px 47px 0px rgba(20, 17, 31, 0.05), 0px 35px 35px 0px rgba(20, 17, 31, 0.09), 0px 9px 19px 0px rgba(20, 17, 31, 0.10)"
        },
				{
          "name": "Medium Dark",
          "slug": "medium-dark",
          "shadow": "0px 66px 18px 0px rgba(20, 17, 31, 0.00), 0px 42px 17px 0px rgba(20, 17, 31, 0.01), 0px 24px 14px 0px rgba(20, 17, 31, 0.05), 0px 10px 10px 0px rgba(20, 17, 31, 0.09), 0px 3px 6px 0px rgba(20, 17, 31, 0.10)"
        },
				{
          "name": "Small Dark",
          "slug": "small-dark",
          "shadow": "0px 16px 4px 0px rgba(20, 17, 31, 0.00), 0px 10px 4px 0px rgba(20, 17, 31, 0.01), 0px 6px 3px 0px rgba(20, 17, 31, 0.05), 0px 3px 3px 0px rgba(20, 17, 31, 0.09), 0px 1px 1px 0px rgba(20, 17, 31, 0.10)"
        },
				{
          "name": "Extra Large Light",
          "slug": "extra-large-light",
          "shadow": "0px 536px 150px 0px rgba(20, 17, 31, 0.00), 0px 343px 137px 0px rgba(20, 17, 31, 0.01), 0px 193px 116px 0px rgba(20, 17, 31, 0.03), 0px 86px 86px 0px rgba(20, 17, 31, 0.04), 0px 21px 47px 0px rgba(20, 17, 31, 0.05)"
        },
				{
          "name": "Large Light",
          "slug": "large-light",
          "shadow": "0px 219px 61px 0px rgba(20, 17, 31, 0.00), 0px 140px 56px 0px rgba(20, 17, 31, 0.01), 0px 79px 47px 0px rgba(20, 17, 31, 0.03), 0px 35px 35px 0px rgba(20, 17, 31, 0.04), 0px 9px 19px 0px rgba(20, 17, 31, 0.05)"
        },
				{
          "name": "Medium Light",
          "slug": "medium-light",
          "shadow": "0px 69px 19px 0px rgba(20, 17, 31, 0.00), 0px 44px 18px 0px rgba(20, 17, 31, 0.01), 0px 25px 15px 0px rgba(20, 17, 31, 0.03), 0px 11px 11px 0px rgba(20, 17, 31, 0.04), 0px 3px 6px 0px rgba(20, 17, 31, 0.05)"
        },
				{
          "name": "Small Light",
          "slug": "small-light",
          "shadow": "0px 16px 5px 0px rgba(20, 17, 31, 0.00), 0px 10px 4px 0px rgba(20, 17, 31, 0.00), 0px 6px 4px 0px rgba(20, 17, 31, 0.02), 0px 3px 3px 0px rgba(20, 17, 31, 0.03), 0px 1px 1px 0px rgba(20, 17, 31, 0.03)"
        }
      ]
    },
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			],
			"blockGap": true,
			"padding": true,
			"margin": true
		},
    "typography": {
			"dropCap": false,
			"defaultFontSizes": false,
			"fluid": true,
			"writingMode": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "slug": "system-fonts",
          "name": "System Fonts"
        },
        {
          "fontFamily": "\"Outfit\", sans-serif",
          "name": "Outfit",
          "slug": "outfit",
          "fontFace": [
            {
              "fontFamily": "Outfit",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontDisplay": "swap",
              "src": ["https://fonts.gstatic.com/s/outfit/v11/QGYyz_MVcBeNP4NjuGObqx1XmO1I4TC0C4G-StheGfs.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "inter",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontDisplay": "swap",
              "src": ["https://fonts.gstatic.com/s/inter/v13/UcCO3FwrK3iLTeHuS_fvQtMwCp50KnMw2boKoduKmMEVuLyfAZ9hjp-Ek-_EeA.woff2"]
            }
          ]
        }
      ],
      "fontSizes": [
        { "slug": "xs", "size": "clamp(0.75rem, 0.7rem + 0.25vw, 0.875rem)", "name": "XS" },
        { "slug": "sm", "size": "clamp(0.875rem, 0.8rem + 0.375vw, 1rem)", "name": "Small" },
        { "slug": "base", "size": "clamp(1rem, 0.95rem + 0.25vw, 1.125rem)", "name": "Base" },
        { "slug": "md", "size": "clamp(1.125rem, 1.05rem + 0.375vw, 1.25rem)", "name": "Medium" },
        { "slug": "lg", "size": "clamp(1.25rem, 1.15rem + 0.5vw, 1.5rem)", "name": "Large" },
        { "slug": "xl", "size": "clamp(1.5rem, 1.35rem + 0.75vw, 1.875rem)", "name": "XL" },
        { "slug": "2xl", "size": "clamp(1.875rem, 1.65rem + 1.125vw, 2.25rem)", "name": "2XL" },
        { "slug": "3xl", "size": "clamp(2.25rem, 2rem + 1.25vw, 3rem)", "name": "3XL" },
        { "slug": "4xl", "size": "clamp(3rem, 2.5rem + 2.5vw, 4.5rem)", "name": "4XL" },
        { "slug": "5xl", "size": "clamp(3.75rem, 3rem + 3.75vw, 6rem)", "name": "5XL" }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "typography": {
      "fontFamily": "var:preset|font-family|inter",
      "fontSize": "var:preset|font-size|base",
      "lineHeight": "1.7"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|outfit",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h1": { "typography": { "fontSize": "var:preset|font-size|4-xl", "fontWeight": "800", "letterSpacing": "-0.02em" } },
      "h2": { "typography": { "fontSize": "var:preset|font-size|3-xl", "fontWeight": "700", "letterSpacing": "-0.01em" } },
      "h3": { "typography": { "fontSize": "var:preset|font-size|2-xl", "fontWeight": "600" } },
      "h4": { "typography": { "fontSize": "var:preset|font-size|xl", "fontWeight": "600" } },
      "h5": { "typography": { "fontSize": "var:preset|font-size|lg", "fontWeight": "600" } },
      "h6": { "typography": { "fontSize": "var:preset|font-size|md", "fontWeight": "600", "textTransform": "uppercase", "letterSpacing": "0.05em" } },
      "link": {
        "color": { "text": "var:preset|color|primary" },
        "typography": { "textDecoration": "none" },
        ":hover": { "typography": { "textDecoration": "underline" } }
      },
      "button": {
        "border": { "radius": "6px" },
        "typography": { "fontWeight": "600" }
      },
      "caption": {
        "typography": { "fontSize": "var:preset|font-size|sm" },
        "color": { "text": "var:preset|color|neutral-600" }
      },
      "cite": {
        "typography": { "fontSize": "var:preset|font-size|sm", "fontStyle": "italic" },
        "color": { "text": "var:preset|color|neutral-500" }
      }
    },
    "blocks": {
      "core/button": {
        "border": { "radius": "6px" },
        "spacing": { "padding": { "top": "0.75rem", "bottom": "0.75rem", "left": "1.5rem", "right": "1.5rem" } }
      },
      "core/image": {
        "border": { "radius": "12px" },
        "shadow": "var:preset|shadow|md"
      },
      "core/table": {
        "border": { "width": "1px", "style": "solid", "color": "var:preset|color|neutral-200" },
        "typography": { "fontSize": "var:preset|font-size|sm" }
      },
      "core/quote": {
        "border": { "left": { "color": "var:preset|color|primary", "width": "4px" } },
        "spacing": { "padding": { "left": "var:preset|spacing|40" } },
        "typography": { "fontSize": "var:preset|font-size|lg", "fontStyle": "italic" }
      },
      "core/code": {
        "color": { "background": "var:preset|color|neutral-100", "text": "var:preset|color|neutral-800" },
        "border": { "radius": "4px" },
        "spacing": { "padding": { "top": "var:preset|spacing|20", "bottom": "var:preset|spacing|20", "left": "var:preset|spacing|30", "right": "var:preset|spacing|30" } },
        "typography": { "fontFamily": "var:preset|font-family|system-fonts", "fontSize": "var:preset|font-size|sm" }
      },
      "core/preformatted": {
        "color": { "background": "var:preset|color|neutral-900", "text": "var:preset|color|neutral-50" },
        "border": { "radius": "8px" },
        "spacing": { "padding": { "top": "var:preset|spacing|30", "bottom": "var:preset|spacing|30", "left": "var:preset|spacing|30", "right": "var:preset|spacing|30" } },
        "typography": { "fontFamily": "var:preset|font-family|system-fonts", "fontSize": "var:preset|font-size|sm" }
      },
      "core/search": {
        "css": ".wp-block-search__input { padding: 12px; border: 1px solid var(--wp--preset--color--neutral-300); border-radius: 6px; } .wp-block-search__button { margin-left: 8px; }"
      },
      "core/separator": {
        "color": { "text": "var:preset|color|neutral-200" }
      }
    }
  },
  "customTemplates": [],
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}
